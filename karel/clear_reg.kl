PROGRAM clear_reg
%COMMENT = 'Clear Register'
%SYSTEM
%NOLOCKGROUP
%NOABORT=ERROR+COMMAND+TPENABLE
%NOPAUSE=ERROR+COMMAND+TPENABLE
%NOPAUSESHFT

VAR
    STATUS      : INTEGER
    return_code : INTEGER

BEGIN

    SET_INT_REG(80, 0, STATUS)
    SET_INT_REG(81, 0, STATUS)
    SET_INT_REG(82, 0, STATUS)
    SET_INT_REG(83, 0, STATUS)

END clear_reg